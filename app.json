{
  "expo": {
    "name": "HMD",
    "slug": "hmd",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-hmd.png",
    "splash": {
      "image": "./assets/splash-hmd.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 3,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-hmd.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.rahmatoktavian.hmd",
      "permissions": [ "CAMERA" ]
    },
    "web": {
      "favicon": "./assets/favicon-hmd.png"
    }
  }
}
